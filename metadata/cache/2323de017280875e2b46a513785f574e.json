    {
      "created": "2023-09-18T19:30:36.179029+00:00",
      "modified": "2023-09-19T02:27:06.416746+00:00",
      "id": 8357021,
      "conceptrecid": "8357020",
      "doi": "10.5281/zenodo.8357021",
